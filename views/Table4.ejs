<!DOCTYPE html>
<html>
<head>
<style>
table,td,tr,th {
  border: 1px solid black;
  border-collapse:collapse;
  padding: 25px;
}


</style>
</head>
<body>

<h2>get_all_Items</h2>
<!-- <p>Use the CSS border property to add a border to the table.</p> -->
<table style="width:100%">
<tr>
    <th>email</th>
    <th>Name</th>
    <th>Details</th>
    <th>Have_a_question</th>
    <th>action</th>
</tr>
  <% for(var i = 0; i <= results.length;i++){
    if(results[i]){ %>
  <tr>
        <td>   <%- results[i].email %>          </td>
        <td>   <%- results[i].Name %>          </td>
        <td>   <%- results[i].Details %>          </td>
        <td>   <%- results[i].Have_a_question %>   </td>
        <td><a href="/edit-item?id=<%- results[i]._id %>&email=<%- results[i].email %>"><button type="button" >Edit</button></a>
        <a href="/delete1?id=<%- results[i]._id %>&email=<%- results[i].email %>"><button type="button" >Delete</button></a></td>




  </tr>
<% }} %>

</table>

</body>
</html>
